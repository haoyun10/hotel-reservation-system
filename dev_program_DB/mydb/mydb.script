CREATE MEMORY TABLE PAYMENT(ROOMNUMBER VARCHAR(50) NOT NULL,STAYINGDATE VARCHAR(20) NOT NULL,ROOMTYPE VARCHAR(50) NOT NULL,AMOUNT INTEGER,STATUS VARCHAR(10),CONSTRAINT ROOMNUMBER_STAYINGDATE PRIMARY KEY(ROOMNUMBER,STAYINGDATE))
CREATE MEMORY TABLE AVAILABLEQTY(DATE VARCHAR(20) NOT NULL,ROOMTYPE VARCHAR(50) NOT NULL,QTY INTEGER,CONSTRAINT DATE_ROOMTYPE PRIMARY KEY(DATE,ROOMTYPE))
CREATE MEMORY TABLE RESERVATION(RESERVATIONNUMBER VARCHAR(50) NOT NULL PRIMARY KEY,STAYINGDATE VARCHAR(20),ROOMTYPE VARCHAR(50) NOT NULL,STATUS VARCHAR(10))
CREATE MEMORY TABLE ROOM(ROOMNUMBER VARCHAR(50) NOT NULL PRIMARY KEY,ROOMTYPE VARCHAR(50) NOT NULL,STAYINGDATE VARCHAR(20))
